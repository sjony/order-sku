<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                         http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="person" class="test.Person" lazy-init="true">
        <property name="name" value="shujiangcheng"/>
        <property name="age" value="25"/>
    </bean>

    <bean id="person1" class="test.Person">
        <property name="name" value="aaa"/>
        <property name="age" value="234234"/>
    </bean>

    <bean id="person2" class="test.Person">
        <property name="name" value="bbb"/>
        <property name="age" value="567567"/>
    </bean>

    <bean id="person4" class="test.Person" lazy-init="false">
        <property name="name" value="111"/>
        <property name="age" value="222"/>
    </bean>

</beans>